load('NE.mat');
addpath('./freezeColors/');
addpath('./EllipsePrj/');
convert_to_matlab;
detect_NE.
A = N1.NE_tensor;
C = N1.NE_center;

freezeColors
S1_clusters = N1.L2_clusters;
for i = 1:1:size(S1_clusters,1)
    S1_clusters(i,:) = S1_clusters(i,:) - C';
end

plot3(S1_clusters(:,1)+C(1),S1_clusters(:,2)+C(2),S1_clusters(:,3)+C(3),'.r','markersize',25);